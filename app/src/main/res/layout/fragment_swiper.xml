<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/nn">


    <Button
        android:id="@+id/btnnotification"
        android:layout_width="148dp"
        android:layout_height="132dp"
        android:layout_marginTop="52dp"
        android:background="@drawable/a__8_"
        android:rotationX="-4"
        app:backgroundTint=""
        app:layout_constraintBottom_toBottomOf="@+id/textView2"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.49"
        app:layout_constraintStart_toEndOf="@+id/textView2"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/btnsetting"
        android:layout_width="148dp"
        android:layout_height="132dp"
        android:layout_marginTop="52dp"
        android:background="@drawable/a__8_"
        android:rotationX="-4"
        app:backgroundTint=""
        app:layout_constraintBottom_toBottomOf="@+id/textView2"
        app:layout_constraintEnd_toStartOf="@+id/textView2"
        app:layout_constraintHorizontal_bias="0.509"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="469dp"
        android:layout_height="199dp"
        android:layout_marginTop="4dp"
        android:background="@drawable/backroudlike_can"
        app:layout_constraintEnd_toEndOf="@+id/btnCancle3"
        app:layout_constraintHorizontal_bias="0.558"
        app:layout_constraintStart_toStartOf="@+id/btnCancle"
        app:layout_constraintTop_toBottomOf="@+id/imageViewAvatar" />

    <Button
        android:id="@+id/btnCancle"
        android:layout_width="213dp"
        android:layout_height="201dp"
        android:background="@drawable/button_cansel"
        android:rotationX="-4"
        app:backgroundTint="#FFFFFF"
        app:layout_constraintEnd_toStartOf="@+id/btnCancle3"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/textView" />

    <Button
        android:id="@+id/btnCancle3"
        android:layout_width="213dp"
        android:layout_height="201dp"
        android:layout_marginEnd="72dp"
        android:background="@drawable/button_like"
        android:rotationX="-4"
        app:backgroundTint=""
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="@+id/textView" />

    <Button
        android:id="@+id/btnBack"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@drawable/back"
        android:backgroundTint="@color/white"
        app:backgroundTint="#F8F8F8"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView"
        app:layout_constraintVertical_bias="0.0" />

    <ImageView
        android:id="@+id/imageViewAvatar"
        android:layout_width="570dp"
        android:layout_height="567dp"
        android:layout_marginTop="32dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView2"
        app:srcCompat="@drawable/avatar" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="175dp"
        android:layout_height="112dp"
        android:layout_marginTop="52dp"
        android:background="@drawable/logo"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

</androidx.constraintlayout.widget.ConstraintLayout>