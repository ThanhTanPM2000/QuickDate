<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.QuickDate" parent="Theme.MaterialComponents.DayNight.NoActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">#EA465B</item>
        <item name="colorPrimaryVariant">@color/white</item>
        <item name="colorOnPrimary">@color/white</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/teal_200</item>
        <item name="colorSecondaryVariant">@color/teal_700</item>
        <item name="colorOnSecondary">@color/black</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor" tools:targetApi="l">?attr/colorDayNightMode</item>
        <!-- Customize your theme here. -->
        <item name="textColor">#222222</item>
        <item name="buttonTabLayoutSelect">#DADADA</item>
        <item name="tintColor">@color/black</item>
        <item name="colorTabLayout">@color/white</item>
        <item name="colorDayNightMode">#DADADA</item>
    </style>
</resources>